{
    "file": "CS_ADULTOS.csv",
    "target_column": "P4_1",
    "columns": ["P2", "P3", "P5", "P6", "P7"],
    "default_save_path": "./resultados_logreg",
    "model_config": {
        "logistic_regression_config": {
            "max_iter": 300,
